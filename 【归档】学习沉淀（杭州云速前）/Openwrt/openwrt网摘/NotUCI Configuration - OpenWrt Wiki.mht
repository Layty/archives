<p>
This get's executed by <code>init</code> every boot: <a href=3D"http://wiki=
.openwrt.org/doc/techref/process.boot#init" class=3D"wikilink1" title=3D"do=
c:techref:process.boot">init</a>
</p><pre class=3D"code">#!/bin/sh
# Copyright (C) 2006 OpenWrt.org

run_scripts() {
        for i in /etc/rc.d/$1*; do
                [ -x $i ] &amp;&amp; $i $2 2&gt;&amp;1
        done | $LOGGER
}

system_config() {
        config_get_bool foreground $1 foreground 0
}

LOGGER=3D"cat"
[ -x /usr/bin/logger ] &amp;&amp; LOGGER=3D"logger -s -p 6 -t sysinit"

. /etc/functions.sh

config_load system
config_foreach system_config system

if [ "$1" =3D "S" -a "$foreground" !=3D "1" ]; then
        run_scripts "$1" "$2" &amp;
else
        run_scripts "$1" "$2"
fi
</pre>

