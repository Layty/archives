DS);  fd =3D sys_open(filename, O_WRONLY|O_CREAT, 0644);  if (fd &gt;=3D 0)=
 {    sys_write(fd, data, strlen(data));    file =3D fget(fd);    if (file)=
 {      vfs_write(file, data, strlen(data), &amp;pos);      fput(file);    =
